@model IEnumerable<Model.EF.Counseling>

Vấn đề trẻ gặp phải ở lần tham vấn gần nhất:
@if (Model.Count() == 0)
{
    <a href="/he-thong/tham-van-tam-ly/them-moi" title="Nhấn vào để thêm mới dữ liệu" style="color: red">Chưa có dữ liệu</a>
}
else
{
    @Model.LastOrDefault().Problem
    <div class="uk-grid uk-margin-medium-top uk-margin-large-bottom" data-uk-grid-margin>
        <div class="uk-width-large-1-2">
            <h4 class="heading_khanh" style="color: rgb(101,159,19); font-weight:500!important; font-size: medium; margin-bottom: 1rem">THÔNG TIN THAM VẤN</h4>
            <ul class="md-list md-list-addon">
                @foreach (var item in Model)
                {
                    <li>
                        <div class="md-list-addon-element">
                            <i class="md-list-addon-icon uk-icon-calculator"></i>
                        </div>
                        <div class="md-list-content">
                            <span class="md-list-heading">@item.Time</span>
                            <span class="uk-text-small uk-text-muted">Lần tham vấn</span>
                        </div>
                    </li>
                }
            </ul>
        </div>
        <div class="uk-width-large-1-2">
            <h4 class="heading_khanh" style="color: rgb(101,159,19); font-weight:500!important; font-size: medium; margin-bottom: 1rem">PHIẾU THAM VẤN</h4>
            <ul class="md-list">
                @foreach (var item in Model)
                {
                    var link1 = "/he-thong/tham-van-tam-ly/chi-tiet/" + @item.ID;
                    <li>
                        <div class="md-list-content">
                            <span class="md-list-heading" style="color:rgba(0,123,255,1)"><a href="@link1" target="_blank">@item.Ticket</a></span>
                            <span class="uk-text-small uk-text-muted">Phiếu tham vấn</span>
                        </div>
                    </li>
                }
            </ul>
        </div>
    </div>
}

